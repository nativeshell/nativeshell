    "version": "0.2.0",
    "configurations": [
        {
            "name": "(Windows) attach",
            "type": "cppvsdbg",
            "request": "attach",
            "processId": "${command:pickProcess}",
        },
        {
            "name": "Flutter: Attach to Process",
            "type": "dart",
            "request": "attach",
            "vmServiceUri": "${command:dart.promptForVmService}",
            "packages": "${workspaceFolder}/nativeshell_demo/",
            "cwd": "${workspaceFolder}/nativeshell_demo/"
        }
    ]
}